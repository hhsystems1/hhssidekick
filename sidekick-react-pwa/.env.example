# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# AI Provider Configuration
# Options: groq (recommended), ollama, openai, anthropic
# Default: groq (fast, free, easy setup)
VITE_AI_PROVIDER=groq

# Groq Configuration (Recommended - Free Tier)
# Get your API key from: https://console.groq.com
VITE_GROQ_API_KEY=

# Ollama Configuration (Local Privacy Option)
VITE_OLLAMA_URL=http://localhost:11434

# Optional: OpenAI API Key (fallback)
VITE_OPENAI_API_KEY=

# Optional: Anthropic API Key (fallback)
VITE_ANTHROPIC_API_KEY=

# Optional: Model overrides per agent type
# If not set, uses defaults from ai-models.ts
#
# For Groq (default):
# VITE_MODEL_REFLECTION=llama-3.1-8b-instant
# VITE_MODEL_STRATEGY=mixtral-8x7b-32768
# VITE_MODEL_SYSTEMS=mixtral-8x7b-32768
# VITE_MODEL_TECHNICAL=llama-3.1-70b-versatile
# VITE_MODEL_CREATIVE=llama-3.1-8b-instant
#
# For Ollama (local):
# VITE_MODEL_REFLECTION=llama3.1:8b
# VITE_MODEL_STRATEGY=qwen2.5:14b
# VITE_MODEL_SYSTEMS=qwen2.5:14b
# VITE_MODEL_TECHNICAL=deepseek-r1:14b
# VITE_MODEL_CREATIVE=llama3.1:8b

# App Environment
VITE_APP_ENV=development

# n8n webhook (legacy - can be removed if fully migrated to agents)
VITE_N8N_WEBHOOK_URL=https://n8n.helpinghandsystems.com/webhook/chat-webhook
